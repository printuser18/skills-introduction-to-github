        "#ASSIGNEMENT operators\n",
        "#=\n",
        "n1=18\n",
        "n2=n1\n",
        "print(n2)\n",
        "#+= n2+=n1-> n2=n2+n1\n",
        "n2+=n1\n",
        "print(n2)\n",
        "# -= n2-=n1-> n2=n2-n1\n",
        "n2=24\n",
        "n2-=n1\n",
        "print(n2)\n",
        "# *= n2*=n1-> n2=n2*n1\n",
        "n2*=n1\n",
        "print(n2)\n",
        "# **=\n",
        "n2**=n1\n",
        "print(n2)\n",
        "# /=\n",
        "n2=45\n",
        "n2/=n1\n",
        "print(n2)\n",
        "# //=\n",
        "n2=45\n",
        "n2//=n1\n",
        "print(n2)\n",
        "#%=\n",
        "n2=22\n",
        "n2%=n1\n",
        "print(n2)\n"
